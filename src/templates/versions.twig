{% for version in versions %}
## {% spaceless %}
    [{{version.name}}]
    {% if version.tag is defined %}
        (https://github.com/{{ owner }}/{{ repo }}/releases/tag/{{ version.tag }})
    {% endif %}
{% endspaceless %}{% if version.date is defined %}- {{version.date}}{% endif %}
{# Line break ! #}
{% if version.nbrCommits > 0 %}
{# Line break ! #}
{% for changes in version.changes %}
{% include 'commits_block.twig' with { commits: changes.commits, name: changes.name, owner: owner, repo: repo } only %}
{% endfor %}
{% endif %}
{% endfor %}
